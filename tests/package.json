{
  "name": "tokens-cypress",
  "version": "1.0.0",
  "description": "cypress",
  "devDependencies": {
    "@apollo/client": "^3.4.16",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^8.7.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-terminal-report": "^3.3.4",
    "diff": "^5.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "he": "^1.2.0",
    "husky": "^4.2.5",
    "istanbul-lib-coverage": "^3.0.0",
    "jahia-reporter": "^0.2.23",
    "lint-staged": "^10.2.11",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "ms": "^2.1.2",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "sass": "^1.52.1",
    "source-map-support": "^0.5.20",
    "supports-color": "^9.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "util": "^0.12.3",
    "webpack": "^5.67.0",
    "yarn": "^1.22.4"
  },
  "resolutions": {
    "**/glob-parent": "^5.1.2"
  },
  "scripts": {
    "instrument": "nyc instrument --compact=false cypress instrumented",
    "e2e:coverage": "yarn instrument && yarn cypress run --config integrationFolder=./instrumented/integration && yarn nyc report --reporter=html --report-dir=./results/coverage && yarn nyc report",
    "e2e:ci": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "e2e:local": "cypress run --reporter reporter/json.js --reporter-options reportsPath=./results/reports/",
    "e2e:debug": "cypress open",
    "lint": "eslint . -c .eslintrc.json --ext .ts"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "cypress/integration",
      "cypress/page-object"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jahia/sitemap.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/Jahia/sitemap/issues"
  },
  "homepage": "https://github.com/Jahia/sitemap#readme"
}
